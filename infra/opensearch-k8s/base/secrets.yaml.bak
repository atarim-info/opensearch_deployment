# opensearch-secrets.yaml
apiVersion: v1
kind: Secret
metadata:
  name: opensearch-credentials
  # namespace: opensearch
type: Opaque
data:
  # Base64 encoded values (echo -n "admin" | base64)
  username: YWRtaW4=
  password: YWRtaW4=

stringData:
  # Security config
  internal-users.yml: |-
    _meta:
      type: "internalusers"
      config_version: 2
    admin:
      hash: "$2y$12$..."  # Generated hash for your password
      reserved: true
      backend_roles:
      - "admin"
      description: "Admin user"
---
apiVersion: v1
kind: Secret
metadata:
  name: opensearch-certificates
type: Opaque
data:
  root-ca.pem: 
  esnode.pem: 
  esnode-key.pem: 
  dashboards.pem: 
  dashboards.key: 
